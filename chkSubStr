import java.util.*;

public class Substr {
    public boolean[] chkSubStr(String[] p, int n, String s) {
        // write code here
        boolean a[]=new boolean[n];
        for (int i = 0; i < p.length; i++) {
            if(s.contains(p[i])) {
                a[i]=true;
            }
            else {
                a[i]=false;
            }
        }
        return a;
    }
}
