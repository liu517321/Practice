import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int cout = 0;
        for(int i = 0;i<n;i++) {
            n = n / 5;
            cout += n;
        }
        System.out.println(cout);
    }
}
